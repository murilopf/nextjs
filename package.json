{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src/",
    "test": "jest",
    "dev": "NODE_ENV=development next -p 8000",
    "build": "NODE_ENV='production' next build ./src",
    "start": "next start ./src",
    "export": "next export ./src",
    "prod": "NODE_ENV='production' next build ./src && next export ./src && serve './out' -p 8080"
  },
  "dependencies": {
    "next": "9.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "next-manifest": "^3.0.1",
    "next-offline": "^4.0.6",
    "next-page-transitions": "^1.0.0-beta.2",
    "next-redux-wrapper": "^3.0.0",
    "lodash": "^4.17.15",
    "glob": "^7.1.6"
  }
}
